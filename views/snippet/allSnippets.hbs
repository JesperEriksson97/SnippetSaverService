<h1>All snippets!</h1>

{{#each allSnippets}}
<div style="max-width: 500px; padding: 20px; border: 9px inset #1C6EA4;
border-radius: 23px;">
    <h3>{{this.label}}</h3>
    <h5> Author: {{this.username}}</h5>
    <textarea name="snippet" rows="15" cols="60" readonly="true" outline="none">{{this.value}}</textarea>
</div>
{{/each}}

{{> backButton}}